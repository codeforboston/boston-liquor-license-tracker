{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "License Info",
    "type": "object",
    "properties": {
      "index": {
        "type": "integer",
        "minimum": 1,
        "description": "Unique sequential index assigned when writing to file."
      },
      "entity_number": {
        "type": "string",
        "pattern": "^[0-9]+$",
        "description": "Number associated with the license in the voting minutes."
      },
        "legal_name": {
            "type": "string"
        },
        "dba": {
            "type": "string"
        },
        "address": {
            "type": "string"
        },
        "zipcode": {
            "type": "string",
            "pattern": "^[0-9]{5}$",
            "description": "5-digit ZIP code extracted from address."
        },
        "license_number": {
            "type": "string",
            "description": "License number assigned by the Licensing Board (prefix LB- followed by digits)."
        },
        "manager": {
            "type": "string"
        },
        "attorney": {
            "type": "string"
        },
        "status": {
            "type": "string",
            "description": "Granted, Deferred, Dismissed, Corrected, etc."
        },
        "category": {
            "type": "string",
            "description": "Holder of, Has applied for, etc."
        },
        "hours": {
            "type": "string",
            "description": "Closing Time, Hours of Operation"
        },
        "details": {
            "type": "string",
            "description": "Description of petition or application"
        },
        "source": {
            "type": "string",
            "description": "The filename of the source chunk"
        }
    },
    "required": [
        "license_number",
        "legal_name"
    ]
}